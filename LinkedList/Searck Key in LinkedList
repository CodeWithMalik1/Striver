public class Main{
static class LinkedList
{
    Node head;
    Node tail;
    static class Node{
        Node next;
        int data;
        public Node(int data){
            this.data=data;
            this.next=null;
        }
    }
        public void addFirst(int data){
            Node newNode=new Node(data);
         if(head==null){
             head=newNode;
             tail=newNode;
         }   
         else{
             newNode.next=head;
             head=newNode;
         }
        }
         
        
        public void printList(){
            if(head==null){
            System.out.println("List is empty");
            return;
                
            }
            Node temp=head;
            while(temp!=null){
                System.out.printf(temp.data+" ");
                temp=temp.next;
            }
            System.out.println();
        }
        public boolean searchKey(int key) {
        if(head==null){
            System.out.println("List is empty");
            return false;
                
            }
        Node temp=head;
        while(temp!=null){
            if(temp.data==key){
                return true;
            }
            temp=temp.next;
        }
        return false;
    }
       
}
	public static void main(String[] args) {
	    LinkedList li=new LinkedList();
	    li.addFirst(10);
	    li.addFirst(30);
	    li.addFirst(40);
	    li.addFirst(20);
	    li.addFirst(900);
	    li.addFirst(200);
	    li.printList();
	    System.out.println(li.searchKey(200));
	    System.out.println(li.searchKey(2000));
	    
	}
}
