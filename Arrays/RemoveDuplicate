public class Main {
    public static int duplicate(int arr[]){
        int i=0,c=1;
        for(int j=1;j<arr.length;j++){
            if(arr[i]!=arr[j]){
                arr[i]=arr[j];
                c++;
            }
        }
        return c;
    }
  public static void main(String[] args) {
      int arr[]={0,0,3,3,3,3,5,5,6,6};
      System.out.println(duplicate(arr));
  }
}
