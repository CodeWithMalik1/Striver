
public class Main
{
    public static int reversePairs(int[] nums) {
        int c=0;
        for(int i=0;i<nums.length;i++){
            for(int j=i+1;j<nums.length;j++){
                if(nums[i]>nums[j]*2){
                    c++;
                }
            }
        }
        return c;
        
    }
	public static void main(String[] args) {
		 int nums[] = {6, 4, 1, 2, 7};
		 System.out.println(reversePairs(nums));
	}
}
