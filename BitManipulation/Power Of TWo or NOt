// Input: n = 1

// Output: true

// Explanation: 20 = 1

// Input: n = 16

// Output: true

// Explanation: 24 = 16

public class Main {
 public static boolean isPowerOfTwo(int n) {
        return n>0 && (n & (n-1))==0;
    }

 public static void main(String[] args) {
     int n=16;
     System.out.println(isPowerOfTwo(n));
     int n1=20;
     System.out.println(isPowerOfTwo(n1));
 }
}
